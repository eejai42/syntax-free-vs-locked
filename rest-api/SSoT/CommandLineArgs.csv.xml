<CommandLineArgs>
  <CommandLineArg>
    <Name>authenticate</Name>
    <Description>Authenticate as a user with Auth0 integration</Description>
    <DataType>bool</DataType>
    <IsOptional>TRUE</IsOptional>
    <IsList>FALSE</IsList>
    <ExampleValue>-authenticate</ExampleValue>
    <SortOrder>1</SortOrder>
  </CommandLineArg>
  <CommandLineArg>
    <Name>whoami</Name>
    <Description>Check who you are currently operating as</Description>
    <DataType>bool</DataType>
    <IsOptional>TRUE</IsOptional>
    <IsList>FALSE</IsList>
    <ExampleValue>whoami</ExampleValue>
    <SortOrder>2</SortOrder>
  </CommandLineArg>
  <CommandLineArg>
    <Name>demoPassword</Name>
    <Description>the user's password</Description>
    <DataType>string</DataType>
    <IsOptional>TRUE</IsOptional>
    <IsList>FALSE</IsList>
    <ExampleValue>pwd123</ExampleValue>
    <SortOrder>3</SortOrder>
  </CommandLineArg>
  <CommandLineArg>
    <Name>bodyData</Name>
    <Description>Raw data provided</Description>
    <DataType>string</DataType>
    <IsOptional>TRUE</IsOptional>
    <IsList>FALSE</IsList>
    <ExampleValue>{'cellâ€¦}</ExampleValue>
    <SortOrder>5</SortOrder>
  </CommandLineArg>
  <CommandLineArg>
    <Name>bodyFile</Name>
    <Description>Path to file to use</Description>
    <DataType>string</DataType>
    <IsOptional>TRUE</IsOptional>
    <IsList>FALSE</IsList>
    <ExampleValue>Cell.json</ExampleValue>
    <SortOrder>6</SortOrder>
  </CommandLineArg>
  <CommandLineArg>
    <Name>runas</Name>
    <Description>Who to run as</Description>
    <DataType>string</DataType>
    <IsOptional>FALSE</IsOptional>
    <IsList>FALSE</IsList>
    <DefaultValue>Guest</DefaultValue>
    <ExampleValue>Admin</ExampleValue>
    <SortOrder>7</SortOrder>
    <Aliases>as</Aliases>
  </CommandLineArg>
  <CommandLineArg>
    <Name>amqps</Name>
    <Description>AMQPS Connection String</Description>
    <DataType>string</DataType>
    <IsOptional>FALSE</IsOptional>
    <IsList>FALSE</IsList>
    <DefaultValue>amqps://smqPublic:smqPublic@effortlessapi-rmq.ssot.me/ej-syntax-locked-vs-syntax-free</DefaultValue>
    <ExampleValue>amqps://smqPublic:smqPublic@effortlessapi-rmq.ssot.me/ej-syntax-locked-vs-syntax-free</ExampleValue>
    <SortOrder>8</SortOrder>
  </CommandLineArg>
  <CommandLineArg>
    <Name>where</Name>
    <Description>Limit the where clause</Description>
    <DataType>string</DataType>
    <IsOptional>TRUE</IsOptional>
    <IsList>FALSE</IsList>
    <ExampleValue>IsActive=1</ExampleValue>
    <SortOrder>9</SortOrder>
  </CommandLineArg>
  <CommandLineArg>
    <Name>output</Name>
    <Description>Output file name</Description>
    <DataType>string</DataType>
    <IsOptional>TRUE</IsOptional>
    <IsList>FALSE</IsList>
    <ExampleValue>output.json</ExampleValue>
    <SortOrder>10</SortOrder>
  </CommandLineArg>
  <CommandLineArg>
    <Name>help</Name>
    <Description>Get help on a given topic</Description>
    <DataType>bool</DataType>
    <IsOptional>TRUE</IsOptional>
    <IsList>FALSE</IsList>
    <DefaultValue>FALSE</DefaultValue>
    <ExampleValue>specific topic</ExampleValue>
    <SortOrder>11</SortOrder>
  </CommandLineArg>
  <CommandLineArg>
    <Name>action</Name>
    <Description>The specific command to execute.</Description>
    <DataType>string</DataType>
    <IsOptional>TRUE</IsOptional>
    <IsList>FALSE</IsList>
    <ExampleValue>AddCell</ExampleValue>
    <SortOrder>12</SortOrder>
  </CommandLineArg>
  <CommandLineArg>
    <Name>maxpages</Name>
    <Description>Maximum number of pages to return in the results</Description>
    <DataType>int</DataType>
    <IsOptional>TRUE</IsOptional>
    <IsList>FALSE</IsList>
    <ExampleValue>-maxpages 3</ExampleValue>
    <SortOrder>13</SortOrder>
  </CommandLineArg>
  <CommandLineArg>
    <Name>view</Name>
    <Description>Which view in the data source to pull data from.</Description>
    <DataType>string</DataType>
    <IsOptional>TRUE</IsOptional>
    <IsList>FALSE</IsList>
    <ExampleValue>-view 'Summary Data'</ExampleValue>
    <SortOrder>14</SortOrder>
  </CommandLineArg>
  <CommandLineArg>
    <Name>apikey</Name>
    <Description>Airtable API key to use for requests.</Description>
    <DataType>string</DataType>
    <IsOptional>TRUE</IsOptional>
    <IsList>FALSE</IsList>
    <ExampleValue>-apikey 'key1234'</ExampleValue>
    <SortOrder>15</SortOrder>
  </CommandLineArg>
  <CommandLineArg>
    <Name>baseid</Name>
    <Description>ID of Airtable base you're querying.</Description>
    <DataType>string</DataType>
    <IsOptional>TRUE</IsOptional>
    <IsList>FALSE</IsList>
    <ExampleValue>-baseid 'app1234'</ExampleValue>
    <SortOrder>16</SortOrder>
  </CommandLineArg>
  <CommandLineArg>
    <Name>projectname</Name>
    <Description>Name of project that is being used will be saved with the Airtable credentials.</Description>
    <DataType>string</DataType>
    <IsOptional>TRUE</IsOptional>
    <IsList>FALSE</IsList>
    <ExampleValue>-projectname 'task-manager'</ExampleValue>
    <SortOrder>17</SortOrder>
  </CommandLineArg>
  <CommandLineArg>
    <Name>makeDefault</Name>
    <Description>Flag to set the email+apikey as default</Description>
    <DataType>bool</DataType>
    <IsOptional>TRUE</IsOptional>
    <IsList>FALSE</IsList>
    <DefaultValue>FALSE</DefaultValue>
    <ExampleValue>-makeDefault user-xyz@ssot.me</ExampleValue>
    <SortOrder>18</SortOrder>
  </CommandLineArg>
  <CommandLineArg>
    <Name>emailAddress</Name>
    <Description>User's email address</Description>
    <DataType>string</DataType>
    <IsOptional>TRUE</IsOptional>
    <IsList>FALSE</IsList>
    <ExampleValue>-emailAddress=your-airtable-user@domain.com</ExampleValue>
    <SortOrder>19</SortOrder>
  </CommandLineArg>
  <CommandLineArg>
    <Name>listStageInfo</Name>
    <Description>Lists the project's available stages</Description>
    <DataType>bool</DataType>
    <IsOptional>TRUE</IsOptional>
    <IsList>FALSE</IsList>
    <ExampleValue>=-listStageInfo</ExampleValue>
  </CommandLineArg>
  <CommandLineArg>
    <Name>stage</Name>
    <Description>A stage for the project</Description>
    <DataType>string</DataType>
    <IsOptional>TRUE</IsOptional>
    <IsList>FALSE</IsList>
    <ExampleValue>-stage develop</ExampleValue>
    <SortOrder>21</SortOrder>
  </CommandLineArg>
  <CommandLineArg>
    <Name>startServer</Name>
    <Description>Start http://localhost:4242 server to respond to services</Description>
    <DataType>bool</DataType>
    <IsOptional>TRUE</IsOptional>
    <IsList>FALSE</IsList>
    <ExampleValue>-startServer</ExampleValue>
    <SortOrder>22</SortOrder>
  </CommandLineArg>
  <CommandLineArg>
    <Name>restEndpoint</Name>
    <Description>The http REST endpoint to connect to in order to get data</Description>
    <DataType>string</DataType>
    <IsOptional>TRUE</IsOptional>
    <IsList>FALSE</IsList>
    <ExampleValue>-restEndpoint https://localhost/</ExampleValue>
    <SortOrder>25</SortOrder>
  </CommandLineArg>
</CommandLineArgs>